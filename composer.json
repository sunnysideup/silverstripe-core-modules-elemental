{
  "name": "sunnysideup/core-modules-elemental",
  "type": "silverstripe-recipe",
  "description": "Core modules for Silverstripe CMS for Elemental",
  "require": {
    "dnadesign/silverstripe-elemental": "^5",
    "dnadesign/silverstripe-elemental-userforms": "^4",
    "dnadesign/silverstripe-elemental-virtual": "^2",
    "nswdpc/silverstripe-elemental-modeladmin": "dev-master",
    "sunnysideup/elemental-edit-me-button": "^4.0",
    "sunnysideup/elemental-switch-tabs": "dev-master",
    "sunnysideup/elemental-template": "dev-master"
  },
  "require-dev": {},
  "suggest": {
    "sunnysideup/core-modules": "dev-master"
  },
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Nicolaas Francken",
      "email": "modules@sunnysideup.co.nz",
      "role": "lead developer"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "prefer": "source",
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "silverstripe/recipe-plugin": true,
      "silverstripe/vendor-plugin": true
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "nswdpc/silverstripe-elemental-modeladmin",
        "version": "master",
        "source": {
          "url": "https://github.com/sunnysideup/silverstripe-elemental-modeladmin",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ]
}
